CC = gcc
CFLAGS = -Wall -pedantic -ansi -g
OBJ = Fibonacci_Series.o
EXEC = runn

$(EXEC) : $(OBJ)
	$(CC) $(OBJ) -o $(EXEC)

Fibonacci_Series.o : Fibonacci_Series.c Fibonacci_Series.h
	$(CC) -c Fibonacci_Series.c $(CFLAGS)

clean:
	rm -rf $(OBJ) $(EXEC)

runVal:
	valgrind ./$(EXEC)cho~